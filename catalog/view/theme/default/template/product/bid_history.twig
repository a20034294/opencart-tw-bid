<table width="100%" border="0" cellpadding="0" cellspacing="0" class="table_record">
    <tbody><tr>
        <td width="25%" align="center" valign="middle">出價時間</td>
        <td width="25%" align="center" valign="middle">出價者</td>
        <td width="25%" align="center" valign="middle">出價方式</td>
        <td width="25%" align="center" valign="middle">出價金額</td>
    </tr>
    {% for history in histories %}
    <tr class="bid_histor_tr">
        <td align="center" valign="middle">{{ history.bid_time }}</td>
        <td align="center" valign="middle">{{ history.bid_user_id }}</td>
        <td align="center" valign="middle">{% if history.bid_auto == 1 %}自動出價{% else %}直接出價{% endif %} </td>
        <td align="center" valign="middle">{{ history.price_now }}</td>
    </tr>
    {% endfor %}

    </tbody>
</table>